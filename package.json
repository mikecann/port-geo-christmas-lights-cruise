{
  "name": "port-geo-xmas-lights",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:ts": "tsc -b -w",
    "dev:frontend": "cross-env CLOUDFLARE_ENV=dev vite --open",
    "dev:backend": "convex dev",
    "typecheck": "tsc -b",
    "predev": "convex dev --until-success && convex dashboard",
    "build": "tsc -b && vite build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "test": "tsc -b && vitest run",
    "test:unit": "bun run test --project unit",
    "test:e2e": "bun run test --project e2e",
    "dev:test": "vitest"
  },
  "dependencies": {
    "@auth/core": "0.41.1",
    "@aws-sdk/client-s3": "^3.919.0",
    "@browserbasehq/stagehand": "^2.5.2",
    "@convex-dev/aggregate": "^0.1.25",
    "@convex-dev/auth": "^0.0.90",
    "@convex-dev/r2": "^0.7.3",
    "@convex-dev/resend": "^0.1.13",
    "@mantine/carousel": "^8.3.5",
    "@mantine/core": "^8.3.5",
    "@mantine/dropzone": "^8.3.5",
    "@mantine/form": "^8.3.5",
    "@mantine/hooks": "^8.3.5",
    "@mantine/notifications": "^8.3.5",
    "@mantine/nprogress": "^8.3.5",
    "@tabler/icons-react": "^3.35.0",
    "@vis.gl/react-google-maps": "^1.6.1",
    "add": "^2.0.6",
    "browser-image-compression": "^2.0.2",
    "convex": "^1.28.0",
    "convex-helpers": "^0.1.104",
    "dotenv": "^17.2.3",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "execa": "^9.6.0",
    "jose": "^6.1.0",
    "playwright": "^1.56.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tree-kill": "^1.2.2",
    "ts-pattern": "^5.9.0",
    "type-route": "^1.1.0",
    "unzipper": "^0.12.3",
    "use-debounce": "^10.0.6",
    "vitest": "^4.0.4",
    "zod": "^3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/unzipper": "^0.10.11",
    "@vitejs/plugin-react": "^5.1.0",
    "convex-test": "^0.0.38",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "wrangler": "^4.45.1"
  }
}
